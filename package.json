{
   "name": "dealer-service",
   "version": "1.0.0",
   "main": "index.js",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "swagger": "tsoa spec-and-routes",
      "prebuild": "npm run swagger",
      "build": "tsc",
      "start": "ts-node src/index.ts",
      "prestart:dev": "npm run swagger",
      "start:dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
      "docker:build": "docker compose -f docker-compose.local.yml build --no-cache",
      "docker:start": "docker compose -f docker-compose.local.yml up -d",
      "docker:stop": "docker compose -f docker-compose.local.yml down",
      "docker:restart": "npm run docker:stop && npm run docker:start",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "nodemonConfig": {
      "watch": [
         "src",
         ".env"
      ],
      "ignore": [
         "src/routes"
      ],
      "ext": "ts",
      "exec": "node -r ts-node/register --env-file=.env src/index.ts"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "description": "",
   "devDependencies": {
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/jsonwebtoken": "^9.0.7",
      "@types/morgan": "^1.9.9",
      "@types/node": "^16.11.10",
      "@types/pg": "^8.11.10",
      "@types/swagger-ui-express": "^4.1.7",
      "concurrently": "^9.1.0",
      "nodemon": "^3.1.7",
      "ts-node": "10.9.1",
      "typescript": "^4.9.5"
   },
   "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^16.4.5",
      "express": "^4.21.1",
      "express-winston": "^4.2.0",
      "helmet": "^8.0.0",
      "jsonwebtoken": "^9.0.2",
      "jwks-rsa": "^3.1.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "swagger-ui-express": "^5.0.1",
      "tsoa": "^6.5.1",
      "typeorm": "0.3.20",
      "typeorm-logger-adaptor": "^1.2.0",
      "winston": "^3.17.0",
      "winston-daily-rotate-file": "^5.0.0"
   }
}
